{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*).(php)",
        "dest": "public/$1.php"
      },
      {
        "src": "/.*",
        "dest": "public/index.php"
      }
    ]
  }
  